{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "wn/lumen-generators": "1.3.*",
        "nordsoftware/lumen-fractal": "1.*.*",
        "nordsoftware/lumen-doctrine-mongodb-odm": "1.4.*",
        "nordsoftware/lumen-elasticsearch": "1.3.*",
        "darkaonline/swagger-lume": "5.5.*",
        "marcin-orlowski/laravel-api-response-builder": "4.*.*",
        "soapbox/laravel-formatter": "2.x",
        "laravel-doctrine/orm": "1.4.*",
        "laravel-doctrine/migrations": "^1.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "illuminate/mail": "^5.1",
        "symfony/property-access": "^2.7",
        "willdurand/hateoas": "^2.8",
        "beberlei/DoctrineExtensions": "^1.0",
        "mongodb/mongodb": "1.2.0-alpha1",
        "alcaeus/mongo-php-adapter": "*",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "provide": {
    "ext-mongo": "1.6.14"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
